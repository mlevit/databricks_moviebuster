  "datasets" : [ {
    "name" : "4bd1e7ea",
    "displayName" : "Rental Fact",
    "query" : "SELECT * FROM identifier(:catalog || '.moviebuster_gold.rental_fact_view')",
    "parameters" : [ {
      "displayName" : "catalog",
      "keyword" : "catalog",
      "dataType" : "STRING",
      "defaultSelection" : {
